{
    "tslint.enable": true,
    //"tslint.configFile": "./tslint.json",
    "workbench.iconTheme": "vscode-icons",
    //"typescript.tsdk": "node_modules/typescript/lib",
    "editor.rulers": [
        132
    ],
    "files.exclude": {
        "bower_components/": true,
        "node_modules/": true,
        "build/": true,
        "coverage/": true,
        "dist/": true,
        "functions/node_modules": true
    },
    "search.exclude": {
        "bower_components/": true,
        "node_modules/": true,
        "build/": true,
        "coverage/": true,
        "dist/": true,
        "functions/node_modules": true
    },
    "cSpell.words": [
        "firestore",
        "notificationclick",
        "notificationclose",
        "onstatechange",
        "onupdatefound"
    ],
    "todohighlight.isEnable": true,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss"
    ],
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/lib/**",
        "**/bower_components/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map"
    ]
}