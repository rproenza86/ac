{
  "name": "atomic-coders",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.12.0",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.14.0",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
