    "fire-deploy-functions": "firebase deploy --only functions"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/material-ui": "^0.21.1",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/redux-devtools-extension": "^2.13.2",
